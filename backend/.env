# Database
DATABASE_URL=sqlite:../data/oddsforge.db

# Football Data API (optional - get from https://www.football-data.org/)
# Free tier: 10 requests per minute
FOOTBALL_DATA_API_KEY=your_api_key_here

# Logging
RUST_LOG=info

# Server
PORT=3000

# Model settings
ELO_K_FACTOR=32
HOME_ADVANTAGE=100

# Prediction settings
PREDICTION_MODEL_VERSION=ensemble_v1.0
CONFIDENCE_THRESHOLD=0.6

# Dataset export settings
MAX_EXPORT_ROWS=10000
EXPORT_PATH=../data/exports